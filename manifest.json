{
 "manifest_version": 3,
 "name": "Wikipedia-PL",
 "description": "Find what you need on the polish version of Wikipedia.",
 "version": "1.0.0",
 "browser_specific_settings": {
  "gecko": {
   "id": "firefox-webextension-search-engine-wikipedia-pl@smykla.com",
   "strict_min_version": "55.0"
  }
 },
 "icons": {
  "32": "/static/icon-32.png",
  "64": "/static/icon-64.png",
  "128": "/static/icon-128.png"
 },
 "chrome_settings_overrides": {
  "search_provider": {
   "name": "Wikipedia PL",
   "search_url": "https://pl.wikipedia.org/w/index.php?title=Special:Search&search={searchTerms}",
   "keyword": "wpl",
   "favicon_url": "/static/wikipedia.ico",
   "is_default": false,
   "encoding": "UTF-8"
  }
 }
}
